<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <el-resolver>com.perficient.pimentomvc.util.JsfCrudELResolver</el-resolver>
    </application>
    <navigation-rule>
        <navigation-case>
            <from-outcome>welcome</from-outcome>
            <to-view-id>/welcomeJSF.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>benefit</managed-bean-name>
        <managed-bean-class>com.perficient.pimentomvc.BenefitController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>benefitJpa</managed-bean-name>
        <managed-bean-class>com.perficient.pimentomvc.BenefitFacade</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <converter>
        <converter-for-class>com.perficient.pimentomvc.Benefit</converter-for-class>
        <converter-class>com.perficient.pimentomvc.BenefitConverter</converter-class>
    </converter>
    <navigation-rule>
        <navigation-case>
            <from-outcome>benefit_create</from-outcome>
            <to-view-id>/jsp/benefit/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>benefit_list</from-outcome>
            <to-view-id>/jsp/benefit/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>benefit_edit</from-outcome>
            <to-view-id>/jsp/benefit/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>benefit_detail</from-outcome>
            <to-view-id>/jsp/benefit/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>broker</managed-bean-name>
        <managed-bean-class>com.perficient.pimentomvc.BrokerController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>brokerJpa</managed-bean-name>
        <managed-bean-class>com.perficient.pimentomvc.BrokerFacade</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <converter>
        <converter-for-class>com.perficient.pimentomvc.Broker</converter-for-class>
        <converter-class>com.perficient.pimentomvc.BrokerConverter</converter-class>
    </converter>
    <navigation-rule>
        <navigation-case>
            <from-outcome>broker_create</from-outcome>
            <to-view-id>/jsp/broker/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>broker_list</from-outcome>
            <to-view-id>/jsp/broker/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>broker_edit</from-outcome>
            <to-view-id>/jsp/broker/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>broker_detail</from-outcome>
            <to-view-id>/jsp/broker/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>groupBenefit</managed-bean-name>
        <managed-bean-class>com.perficient.pimentomvc.GroupBenefitController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>groupBenefitJpa</managed-bean-name>
        <managed-bean-class>com.perficient.pimentomvc.GroupBenefitFacade</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <converter>
        <converter-for-class>com.perficient.pimentomvc.GroupBenefit</converter-for-class>
        <converter-class>com.perficient.pimentomvc.GroupBenefitConverter</converter-class>
    </converter>
    <navigation-rule>
        <navigation-case>
            <from-outcome>groupBenefit_create</from-outcome>
            <to-view-id>/jsp/groupBenefit/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>groupBenefit_list</from-outcome>
            <to-view-id>/jsp/groupBenefit/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
