            <to-view-id>/jsp/groupBenefit/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>groupBenefit_detail</from-outcome>
            <to-view-id>/jsp/groupBenefit/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>groups</managed-bean-name>
        <managed-bean-class>com.perficient.pimentomvc.GroupsController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>groupsJpa</managed-bean-name>
        <managed-bean-class>com.perficient.pimentomvc.GroupsFacade</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <converter>
        <converter-for-class>com.perficient.pimentomvc.Groups</converter-for-class>
        <converter-class>com.perficient.pimentomvc.GroupsConverter</converter-class>
    </converter>
    <navigation-rule>
        <navigation-case>
            <from-outcome>groups_create</from-outcome>
            <to-view-id>/jsp/groups/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>groups_list</from-outcome>
            <to-view-id>/jsp/groups/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>groups_edit</from-outcome>
            <to-view-id>/jsp/groups/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>groups_detail</from-outcome>
            <to-view-id>/jsp/groups/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
